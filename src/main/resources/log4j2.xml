<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="DEBUG">
  <Appenders>
    <Console name="LogToConsole" target="SYSTEM_OUT">
      <PatternLayout
        pattern="%d{HH:mm:ss.SSS}    [%t]    %-5level    %logger{36}     message:%msg%n"/>
    </Console>
  </Appenders>
  <Loggers>
    <root level="all">
      <appender-ref ref="LogToConsole"/>
    </root>
  </Loggers>
</Configuration>